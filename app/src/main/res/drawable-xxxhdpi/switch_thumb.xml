<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >



    <item android:state_checked="true">
        <shape android:shape="rectangle"
            android:visible="true"
            android:dither="true"
            android:useLevel="false">
            <size android:width="@dimen/iperf_swtich_tumblr_width"
                android:height="@dimen/iperf_swtich_tumblr_height" />
            <corners android:radius="@dimen/iperf_switch_radius"/>
            <solid android:color="@color/iperf_switch_thumb_off_color" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle"
            android:visible="true"
            android:dither="true"
            android:useLevel="false">
            <size android:width="@dimen/iperf_swtich_tumblr_width"
                android:height="@dimen/iperf_swtich_tumblr_height" />
            <corners android:radius="@dimen/iperf_switch_radius"/>
            <solid android:color="@color/iperf_switch_thumb_on_color" />
        </shape>
    </item>


</selector>